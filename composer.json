{
   "name": "rovota/embla",
   "version": "1.0.0",
   "type": "library",
   "description": "A lightweight component library, helping you to build modern interfaces more efficiently.",
   "keywords": ["components", "interface", "php", "rovota", "modern", "web"],
   "homepage": "https://github.com/Rovota/Embla",
   "license": "MIT",
   "authors": [
      {
         "name": "Software Department",
         "email": "developers@rovota.com"
      }
   ],
   "support": {
      "email": "support@rovota.com",
      "docs": "https://rovota.gitbook.io/embla"
   },
   "repositories": [
      {
         "type": "path",
         "url": "../../Rovota/Framework"
      }
   ],
   "require": {
      "php-64bit": "^8.3.0",
      "rovota/framework": "*"
   },
   "autoload": {
      "psr-4": {
         "Rovota\\Embla\\": "src/"
      },
      "files": ["src/functions.php"]
   },
   "config": {
      "sort-packages": true,
      "optimize-autoloader": true
   }
}